function toTitleCase(str) {
  return str.replace(
    /\w\S*/g,
    function(txt) {
      return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
    }
  );
}

let pack = game.packs.get(packName);
let index = pack.index ?? (await pack.getIndex());
var itemArray = [];
let itemID = index.forEach(element => itemArray.push(element._id));
for (let i = 0; i < itemArray.length; i++) {
	let finalItem = await pack.getDocument(itemArray[i]);
	var lowerName = finalItem.name.toLowerCase().replaceAll("'", "").replaceAll("-", "").replaceAll("/", "").replaceAll(" ", "");
	if(spellLists[lowerName] !== undefined){
		var sortedClasses = spellLists[lowerName].split(",").sort().join(", ");
		if(finalItem.system.source.includes(" "+openBracket + toTitleCase(sortedClasses)+ closeBracket) == false){
			finalItem.update({ "system.source" : finalItem.system.source + " "+openBracket + toTitleCase(sortedClasses)+ closeBracket})
		}
	}
}
